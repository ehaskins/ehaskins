{{> _header}}
<h2>{{title}}</h2>
<div>
    {{#each collections.posts}}
        <div class="post">
			<div class="post-header">
				<div class="post-title"><a href="{{link this.path}}">{{title}}</a></div>
				<div class="post-metadata">{{postInfo}}</div>
			</div>    
			<div class="post-excerpt">
				{{{this.excerpt}}}
			</div>
        </div>
    {{/each}}
</div>
{{> _footer}}
